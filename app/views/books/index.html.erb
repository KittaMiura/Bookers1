<body>
  <h1>Books</h1>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Books</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>

    </tbody>
  </table>
  <h2>New Book</h2>
  <%= form_with model: @book do |f| %>

  <label>Title</label>
  <%= f.text_field :title %>
  
  <label>Body</label>
  <%= f.text_area :body %>
  
  <%= f.submit 'Create Book' %>

  <% end %>
</body>